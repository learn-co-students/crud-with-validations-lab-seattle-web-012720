<h1>All Songs</h1>
<table>
    <thead>
        <tr>
            <th>Title</th>
            <th>Artist</th> 
        </tr>
  <thead>
  <% @songs.each do |song| %>
    <tbody>
        <tr>
            <td><%= link_to song.title, song %></td>
            <td> <%= song.artist_name %></td>
        </tr>
        <% end %>
    </tbody>
</table>
